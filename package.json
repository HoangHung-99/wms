{
  "name": "wms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy": "gulp",
    "cr-icon": "yo rn-toolbox:assets --icon ./src/assets/icons/1024.png",
    "android:dev": "yarn copy dev && yarn android",
    "android:qa": "yarn copy qa && yarn android",
    "android:prod": "yarn copy prod && yarn android",
    "android": "npx react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd .. && yarn android",
    "ios": "npx react-native run-ios",
    "ios:clean": "cd ios && xcodebuild clean && cd .. && npx react-native run-ios",
    "ios:clean-all": "rm -rf ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "buildAPK:local": "yarn copy local && cd android && ./gradlew assembleRelease",
    "buildAPK:dev": "yarn copy dev && cd android && ./gradlew assembleRelease",
    "buildAPK:demo": "yarn copy demo && cd android && ./gradlew assembleRelease",
    "buildAPK:prod": "yarn copy prod && cd android && ./gradlew assembleRelease",
    "buildAPK:qa": "yarn copy qa && cd android && ./gradlew assembleRelease",
    "pod-i": "npx pod-install ios",
    "ip6": "npx react-native run-ios --simulator='iPhone 6'",
    "ip7": "npx react-native run-ios --simulator='iPhone 7'",
    "ip7-p": "npx react-native run-ios --simulator='iPhone 7 Plus'",
    "ip8": "npx react-native run-ios --simulator='iPhone 8'",
    "ip8-p": "npx react-native run-ios --simulator='iPhone 8 Plus'",
    "ipx": "npx react-native run-ios --simulator='iPhone X'",
    "buildAPK": "cd android && ./gradlew assembleRelease",
    "buildAAB": "cd android && ./gradlew bundleRelease",
    "buildRelease": "cd android && ./gradlew bundleRelease",
    "iphone": "cd ios && xcodebuild clean && cd .. && npx react-native run-ios --device 'X30 iPhone'",
    "ios:st-ip": "cd ios && xcodebuild clean && cd .. && npx react-native run-ios --udid ",
    "ios:device-list": "instruments -s devices",
    "anddroid:device-list": "adb devices",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clear": "watchman watch-del-all && yarn start -- --reset-cache",
    "clear-metro": "rm -rf /tmp/metro-*",
    "ios:remove": "rm -rf ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "clean-cache": "yarn cache clean",
    "st-com": "yarn add react-native-vector-icons @react-native-community/async-storage @react-native-community/datetimepicker events moment moment-timezone",
    "stdev-com": "yarn add @types/node @types/events @types/moment @types/moment-timezone -D",
    "st-nav": "yarn add @react-navigation/native react-native-reanimated react-native-gesture-handler react-native-screens react-native-safe-area-context @react-native-community/masked-view @react-navigation/stack @react-navigation/drawer",
    "st-babel": "yarn add @babel/plugin-proposal-decorators babel-plugin-module-resolver babel-plugin-transform-remove-console -D",
    "st-mobx": "yarn add mobx mobx-react"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/datetimepicker": "^6.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/toolbar-android": "^0.2.1",
    "@react-native-firebase/app": "^15.1.1",
    "@react-native-firebase/messaging": "^15.1.1",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/drawer": "^6.4.3",
    "@react-navigation/material-top-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/lodash": "^4.14.182",
    "appcenter": "^4.4.4",
    "appcenter-analytics": "^4.4.4",
    "appcenter-crashes": "^4.4.4",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "events": "^3.3.0",
    "i18next": "^21.8.13",
    "json2typescript": "^1.5.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "native-base": "^3.4.8",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-barcode-mask": "^1.2.4",
    "react-native-calendars": "^1.1285.0",
    "react-native-camera": "^4.2.1",
    "react-native-code-push": "^7.0.4",
    "react-native-device-info": "^9.0.2",
    "react-native-elements": "^3.4.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.5.0",
    "react-native-image-picker": "^4.8.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^4.12.2",
    "react-native-permissions": "^3.4.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.14.1",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "util": "^0.12.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.6",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/events": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "@types/node": "^18.0.3",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.71.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
